<div class="row wrapper border-bottom white-bg page-heading" xmlns="http://www.w3.org/1999/html"> 
   <div class="wrapper wrapper-content animated fadeInRight"> 
    <div class="row"> 
     <div class="col-lg-12"> 
      <div class="ibox"> 
       <div class="ibox-title"> 
        <h3><span class="text-navy">任务添加</span></h3> 
       </div> 
       <div class="ibox-content"> 
        <table class="table table-stripped toggle-arrow-tiny footable" data-page-size="10"> 
         <thead> 
          <tr> 
           <th data-toggle="true">任务ID</th> 
           <th data-hide="all">提交节点组</th> 
           <th data-hide="phone,tablet">重复时间间隔</th> 
           <th data-hide="all">重复次数</th> 
           <th data-hide="all">优先级</th> 
          </tr> 
         </thead> 
         <tbody>
          <tr> 
            <td>$page</td>
            <td>12</td>
            <td>12</td>
            <td>12</td>
            <td>12</td>
          </tr> 
         </tbody> 
        </table> 
        <div class="paginationbar"> 
         <ul class='pagination pagination-centered'>
          #if($page.firstPage) 
          <li class="disabled"><a>首页</a></li> 
          #else 
          <li><a href="$page.url">首页</a></li> 
          #end 
          #if($!page.hasPreviousPage) 
          <li class="disabled"><a>&laquo;</a></li> 
          #else 
          <li><a href="$page.url" class="active">&laquo;</a></li> 
          #end 
          #foreach($item in $page.items) 
             #if($item.current) 
	          <li class="disabled"><a>$${item.number}</a></li> 
	         #else 
	          <li><a href="$page.url">$${item.number}</a></li>
	         #end 
	      #end 
	      #if($!page.hasNextPage) 
	          <li class="disabled"><a>&raquo;</a></li> 
	      #else 
	          <li><a href="$page.url">&raquo;</a></li> 
	      #end 
	      #if($page.lastPage) 
	          <li class="disabled"><a>末页</a></li>
	      #else 
	          <li><a href="$page.url">末页</a></li> 
	      #end 
         </ul> 
        </div> 
       </div> 
      </div> 
     </div> 
    </div> 
   </div> 
  </div> 